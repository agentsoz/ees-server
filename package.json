{
  "name": "ees-server",
  "version": "1.0.0",
  "description": "Emergency Evacuation Simulator (EES) - Web Server",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:dev:watch": "webpack --mode development --watch",
    "server": "cd dist && node main.js",
    "server:watch": "cd dist && nodemon main.js --watch .",
    "dev": "npm run build:dev && npm-run-all --parallel build:dev:watch server:watch",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentsoz/ees-server.git"
  },
  "author": "Dhi Singh <dhi.singh@gmail.com>",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/agentsoz/ees-server/issues"
  },
  "homepage": "https://github.com/agentsoz/ees-server#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "nodemon": "^2.0.2",
    "now": "^11.5.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@mapbox/mbtiles": "^0.11.0",
    "@mapbox/tilelive": "^6.1.0",
    "cors": "^2.8.5",
    "download": "^7.1.0",
    "epsg": "^0.5.0",
    "event-stream": "^3.3.5",
    "express": "^4.17.1",
    "global-mercator": "^3.1.0",
    "pixl-xml": "^1.0.13",
    "proj4": "^2.6.0",
    "reproject": "^1.2.5"
  },
  "now": {
    "public": true,
    "name": "ees-server",
    "alias": "ees-server"
  }
}
